{
	"name": "RunBatch",
	"properties": {
		"activities": [
			{
				"name": "testPipeline",
				"type": "Custom",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"command": "/bin/bash -c \"docker run azurebatchpoc.azurecr.io/batchapp4 --mount type=bind,source=\"$(pwd)\"/activity.json=/activity.json,readonly\"",
					"resourceLinkedService": {
						"referenceName": "AzureBlobBatch",
						"type": "LinkedServiceReference"
					},
					"folderPath": "input",
					"referenceObjects": {
						"linkedServices": [
							{
								"referenceName": "AzureSqlDatabase1",
								"type": "LinkedServiceReference",
								"parameters": {
									"KeyVaultSecretName": {
										"value": "@pipeline().globalParameters.SQLServerKeyVaultSecretName",
										"type": "Expression"
									},
									"FQDN": {
										"value": "@pipeline().globalParameters.SQLServerFQDN",
										"type": "Expression"
									},
									"DBName": {
										"value": "@pipeline().globalParameters.SQLServerDBName",
										"type": "Expression"
									},
									"Username": {
										"value": "@pipeline().globalParameters.SQLServerUsername",
										"type": "Expression"
									}
								}
							},
							{
								"referenceName": "Sftp1",
								"type": "LinkedServiceReference"
							},
							{
								"referenceName": "export_blob",
								"type": "LinkedServiceReference"
							},
							{
								"referenceName": "archive_blob",
								"type": "LinkedServiceReference"
							}
						],
						"datasets": [
							{
								"referenceName": "sFTPRawDataCSV",
								"type": "DatasetReference"
							},
							{
								"referenceName": "AzureBlobArchived",
								"type": "DatasetReference"
							},
							{
								"referenceName": "AzureSQLUsersTable",
								"type": "DatasetReference"
							}
						]
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureBatchPOC",
					"type": "LinkedServiceReference"
				}
			}
		],
		"annotations": [],
		"lastPublishTime": "2022-09-01T14:14:09Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}